# --------------------------------------------------------------------------------
# 1. Fibre Channel Module Entry info
# Polls the .1.3.6.1.4.1.122.8501 oid, FIBRE-CHANNEL-FE-MIB::fcFeModuleTable
# "A table that contains, one entry for each module in the Fabric Element, information of the modules."
# Reads:
#  .1.3.6.1.4.1.122.8501.3.101.4.1.0, location, String
#  .1.3.6.1.4.1.122.8501.3.101.1.1.0, model number, String
#  .1.3.6.1.4.1.122.8501.3.101.6.1.1.5.1, lamp hours, Counter32

def inventory_sony_vp(checkname, info):
  inventory = []
  for index, descr, objectid in info:
    if state == "2" or state == "3":
      inventory.append( (index, None) )
  return inventory

def check_sony_vp(item, _no_params, info):
  for index, descr, objectid in info:
    if index == item:
      if status == "3" or status == "9":
        return (0, "OK - " + speed + "mps")
      else:
        return (2, "CRITICAL - " + speed + "mps")
  return (3, "UNKNOWN - Port info not found")

check_info["sony_vp"] = \
      (check_sony_vp, "Port %s", 0, inventory_sony_vp)

snmp_info["sony_vp"] = ( ".1.3.6.1.4.1.122.8501", [ ".3.101.4.1.0", ".3.101.1.1.0", ".3.101.6.1.1.5.1" ] )
snmp_scan_functions['sony_vp'] = \
  lambda oid: oid(".1.3.6.1.2.1.1.2.0").startswith(".1.3.6.1.4.1.122.8501")